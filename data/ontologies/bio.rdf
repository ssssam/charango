  <rdf:Description rdf:about="http://purl.org/vocab/bio/0.1/Marriage">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Marriage</rdfs:label>

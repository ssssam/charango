  <rdf:Description rdf:about="http://purl.org/vocab/bio/0.1/Event">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Event</rdfs:label>
    <rdfs:comment xml:lang="en">An event is an occurrence that brings about a change in the state of affairs for one or more people and/or other agents. Events are assumed to occur over a period of time and may not have precise start and end points.</rdfs:comment>
    <vann:usageNote xml:lang="en">This class is intended to describe biographical events, i.e. events in the life of a person.</vann:usageNote>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
    <rdfs:subClassOf rdf:resource="http://sw.opencyc.org/2009/04/07/concept/en/Event"/>
    <rdfs:subClassOf rdf:resource="http://linkedevents.org/ontology/Event"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/dcmitype/Event"/>

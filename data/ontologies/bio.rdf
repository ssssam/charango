  <rdf:Description rdf:about="http://purl.org/vocab/bio/0.1/Annulment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Annulment</rdfs:label>
    <rdfs:comment xml:lang="en">The event of declaring a marriage void from the beginning as though it never existed.</rdfs:comment>
